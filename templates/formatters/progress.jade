#progress
  span
    span.seed
      i.icon-random
      span.value= seed

    span.time
      i.icon-time
      span.value 0s

    span.all
      span.value 0
      /
      span.total 0

    ul
      each chr,state in chars
        li(class=state, data-state=state)
          span.value 0
          span.symbol= ' ' + state


